### file: t1.baseline.sysbench.tiflow
help = SysBench baseline on t1 deployment
abbr = t1.base.sysben|sb

flow = t1.conf.deploy : t1.conf.sysben : 2sb.prepare env=t1-baseline-sb : repeat 2sb.run.base.sysben [[times]]

[arg]
hosts|h = ''
times = 5
threads-list|threads|t = 50,100,150,200,400,600
tags|tag = t1

[arg2env]
bench.threads-list = threads-list
# could instead by `deploy.aut.conf <hosts>`
deploy.hosts = hosts
# all keys of `bench.tag.*` will be recorded
bench.tag.by-user = tags
