help = compare the performance between with/without tikv cpu limitation

[arg]
cpu-limit|limit|l = ''

[env]
bench.limit-tikv-cpu = cpu-limit

[flow/]
bench.set-tag tikv-cpu-limit,unlimited : app.deploy+bench
deploy.set.tikv.res cpu_quota [[bench.limit-tikv-cpu]] : bench.set-tag tikv-cpu-limit,[[bench.limit-tikv-cpu]] : app.deploy+bench
