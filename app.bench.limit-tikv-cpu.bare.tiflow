help = compare the performance between with/without tikv cpu limitation

[arg]
cpu-limit|limit|l = ''
workload|wl = ''

[arg2env]
bench.limit-tikv-cpu = cpu-limit
bench.workload = workload

[flow/]
bench.set-tag tikv-cpu-limit,unlimited
deploy.auto depose-env=true
app.bench.2step.run

deploy.set.tikv.res cpu_quota [[bench.limit-tikv-cpu]]
bench.set-tag tikv-cpu-limit,[[bench.limit-tikv-cpu]]
deploy.auto depose-env=true
app.bench.2step.run
