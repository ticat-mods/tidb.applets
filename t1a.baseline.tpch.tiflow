### file: t1a.baseline.tpch.tiflow
help = tpch baseline on t1a deployment
abbr = t1a.base.tpch

flow = t1a.conf.deploy : t1.conf.tpch : 2sb.prepare env=t1a-baseline-tpch : repeat 2sb.run.baseline.tpch [[times]]

[arg]
hosts|h = ''
times = 5
threads-list|threads|t = 1,2,3
tags|tag = t1a

[arg2env]
bench.threads-list = threads-list
# could instead by `deploy.aut.conf <hosts>`
deploy.hosts = hosts
# all keys of `bench.tag.*` will be recorded
bench.tag.by-user = tags
