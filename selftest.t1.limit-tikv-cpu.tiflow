help = test `t1.limit-tikv-cpu` with minimun data
abbr = selftest.t1.limit-kv-cpu|lkvu

tag = selftest

[arg]
hosts|host|h = 127.0.0.1
wait-secs-before-backup|w = 0

[arg2env]
