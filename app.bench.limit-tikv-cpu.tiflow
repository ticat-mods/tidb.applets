help = compare the performance between with/without tikv cpu limitation
abbr = app.ben.limit-kv-cpu

[arg]
hosts|host|h = ''
cpu-limit|limit|l = 400%

[arg2env]
deploy.hosts = hosts
bench.limit-tikv-cpu = cpu-limit

[flow/]
app.bench.2step.prepares
tpcc.config : app.bench.limit-tikv-cpu.bare
ycsb.config : app.bench.limit-tikv-cpu.bare
sysbench.config : app.bench.limit-tikv-cpu.bare
bench.result
