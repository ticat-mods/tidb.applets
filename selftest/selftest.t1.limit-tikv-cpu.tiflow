help = test `t1.limit-tikv-cpu` with minimun data
abbr = selftest.t1.limit-kv-cpu|limit-kv-cpu|lkvu

tag = selftest

[arg]
hosts|host|h = 127.0.0.1

[arg2env]
deploy.hosts = hosts

[val2env]
bench.prepare.wait-secs-before-backup = 0
bench.tag.t1.limit-tikv-cpu = selftest
bench.run.times = 1
bench.tpcc.threads-list = 1
bench.tikv-cpu-limit-list = 400%

[flow/]
tpcc.conf : sysbench.conf
t1.limit-tikv-cpu
br.ssh.drop
bench.result.last
