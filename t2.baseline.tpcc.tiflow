### file: t2.baseline.tpcc.tiflow
help = tpcc baseline on t2 deployment
abbr = t2.base.tpcc

flow = t2.conf.deploy : t2.conf.tpcc : 2sb.prepare env=t2-baseline-tpcc : repeat 2sb.run.baseline.tpcc [[times]]

args.auto = hosts,threads-list

[arg]
times = 5
tags|tag = t2

[arg2env]
# all keys of `bench.tag.*` will be recorded
bench.tag.by-user = tags
