help = compare the performance between w/wo tikv cpu limitation
abbr = app.limit-kv-cpu
trivial = 1

[arg]
cpu-limit|limit|l = 750%
workload|wl = ''

[arg2env]
bench.limit-tikv-cpu = cpu-limit
bench.workload = workload

[flow/]
bench.set-tag tikv-cpu-unlimited
deploy.auto depose-env=true
app.2step.run

deploy.set.tikv.res cpu_quota [[bench.limit-tikv-cpu]]
bench.set-tag tikv-cpu-[[bench.limit-tikv-cpu]]
deploy.auto depose-env=true
app.2step.run
