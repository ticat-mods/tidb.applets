help = compare the performance between w/wo tikv cpu limitation
abbr = 2step.run.limit-kv-cpu|lkvu
trivial = 1

[arg]
cpu-limit|limit|l = ''
env-snapshot-name|env = 2sb-lkvu

[arg2env]
2-step-bench.env.snapshot = env-snapshot-name

[flow/]
bench.set-tag tikv-cpu-[[cpu-limit]]
deploy.set.tikv.res cpu_quota [[cpu-limit]]
deploy.auto depose-env=true
2-step-bench.run
